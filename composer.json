{
  "name": "alexconrad/easy-mysql",
  "type": "library",
  "description": "Most used queries in mysql",
  "keywords": [
    "mysql easy"
  ],
  "homepage": "https://github.com/alexconrad/easy-mysql",
  "license": "MIT",
  "authors": [
    {
      "name": "Tamcencu Alexandru",
      "email": "conrad1@gmail.com"
    }
  ],
  "require": {
    "php": "8.0.*",
    "myclabs/php-enum": "~1.6",
    "php-di/php-di": "^6.3",
    "psr/container": "1.0.*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.3",
    "ext-mysqli": "*",
    "ext-pdo": "*",
    "ext-json": "*"
  },
  "autoload": {
    "psr-4": {
      "EasyMysql\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "EasyMysqlTests\\": "test/"
    }
  },
  "config": {
    "disable-tls": true,
    "secure-http": false
  }
}
