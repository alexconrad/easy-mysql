{
  "name": "alexconrad/easy-mysql",
  "description": "Most used queries in mysql",
  "type": "library",
  "require": {
    "php": "8.0.*",
    "myclabs/php-enum": "~1.6.1",
    "php-di/php-di": "^6.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.3"
  },
  "autoload": {
    "psr-4": {
      "EasyMysql\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "EasyMysql\\": "test/"
    }
  },
  "config": {
    "disable-tls": true,
    "secure-http": false
  }
}
