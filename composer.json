{
  "name": "alexconrad/easy-mysql",
  "description": "Most used queries in mysql",
  "type": "library",
  "require": {
    "php": "8.0.*",
    "myclabs/php-enum": "~1.6.1",
    "php-di/php-di": "^6.3",
    "psr/container": "1.0.*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.3",
    "ext-mysqli": "*",
    "ext-pdo": "*"
  },
  "autoload": {
    "psr-4": {
      "EasyMysql\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "EasyMysqlTests\\": "test/"
    }
  },
  "config": {
    "disable-tls": true,
    "secure-http": false
  }
}
